{
  "schemaVersion": 1,
  "id": "bushierflowers",
  "version": "${version}",

  "name": "Bushier Flowers",
  "description": "Bonemealing flowers grows them into multiple inside of one block. Allows duplication like double tall flowers do.",
  "authors": [
    "Pandarix"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/bushier-flowers",
    "sources": "https://github.com/Pandarix/BushyFlowers"
  },

  "license": "MIT",
  "icon": "assets/bushierflowers/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.Pandarix.bushierflowers.BushierFlowers"
    ],
    "client": [
      "net.Pandarix.bushierflowers.BushierFlowersClient"
    ],
    "fabric-datagen": [
      "net.Pandarix.bushierflowers.BushierFlowersDataGenerator"
    ]
  },
  "mixins": [
    "bushierflowers.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric-api": "*",
    "minecraft": "~1.20",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
